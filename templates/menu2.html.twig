
<div id="message">
    <span>Choisi une catégorie et sélectionne tout ce dont tu as besoin !</span>
</div>


    <ul id="menu-accordeon">
        {% for categorie in categories %}
      <li><a>{{ categorie.NomCat }}</a>
        {% for sousCategorie in subCategories %}
          {% if sousCategorie.NumsousCat matches "/" ~ categorie.NumCat ~ "/" %}
        <ul>
           <li><a href="/products.html.twig">{{ sousCategorie.NomsousCat }}</a></li>
        </ul>
        {% endif %}
        {% endfor %}
      </li>
      {% endfor %}

    </ul>
