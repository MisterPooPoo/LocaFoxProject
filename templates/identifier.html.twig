<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>LocaFox</title>
  <meta charset="utf-8">
  <title>La societe</title>

  <link rel="stylesheet" media="screen" href="https://fontlibrary.org/face/carlito" type="text/css"/>
  <link rel="stylesheet" href="/css/stylepopup.css"/>
</head>

<body>
  <header>
    <img src="/images/bannierepopup.png">
  </header>

  <div id="corps">
    <h1>Identification</h1>
    <div id="text">
      <fieldset>
        <form id="formulaire" method="POST" >
          <label>Adresse e-mail</label>
          {% if errors.email is defined %}
          <div>
            {{ errors.email }}
          </div>
          {% endif %}
          <input type="email" name="email" value="{{ formData.email }}"><br><br>
          <label>Mot de passe</label>
          {% if errors.password is defined %}
          <div>
            {{ errors.password }}
          </div>
          {% endif %}
          <input type="password" name="password" value="{{  formData.password }}">
          <input type="submit" value="valider">
        </form>
      </fieldset>
    </div>
  </div>

  {% if closeZoombox %}
  <script type="text/javascript">
  parent.document.location.href = "/index.php";
  parent.$.zoombox.close();
  </script>
  {% endif %}
  
</body>
</html>
